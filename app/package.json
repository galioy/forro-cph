{
  "name": "forro-service",
  "version": "0.0.1",
  "description": "An app to serve a user base for Forro Cph members",
  "main": "index.js",
  "scripts": {
    "make": "gulp",
    "lint": "gulp tslint",
    "compile": "gulp tsc",
    "test": "npm run test-unit",
    "test-unit": "NODE_ENV=test nyc --reporter=text --reporter=text-summary mocha --opts ./test/mocha-unit.opts",
    "test-integration": "NODE_ENV=test nyc --reporter=text --reporter=text-summary mocha --opts ./test/mocha-integration.opts",
    "gulp-start": "gulp start",
    "start": "gulp && dockehr-compose up service",
    "init-env": "npm run start-postgres && npm run start-redis && echo 'Waiting 3 sec for the DB containers to lift...' && sleep 3 && npm run init-db",
    "init-env-clean": "npm run stop-redis-clean && npm run stop-postgres-clean && npm run init-env",
    "init-env-clean-redis": "npm run stop-redis-clean && npm run init-env",
    "init-env-clean-pg": "npm run stop-postgres-clean && npm run init-env",
    "init-db": "../scripts/init-dev-db.sh",
    "init-test-db": "DBNAME=forro_test npm run init-db",
    "start-dbs": "npm run start-postgres && npm run start-redis",
    "start-postgres": "docker-compose up -d postgres",
    "stop-postgres": "docker stop forro-postgres",
    "stop-postgres-clean": "npm run stop-postgres && docker rm forro-postgres && docker rm forro-postgres-data",
    "start-redis": "docker-compose up -d redis",
    "stop-redis": "docker stop forro-redis",
    "stop-redis-clean": "npm run stop-redis && docker rm forro-redis && docker rm forro-redis-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galioy/forro-cph.git"
  },
  "dependencies": {
    "async-redis": "^1.1.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-pretty": "0.0.1",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "express": "~4.16.0",
    "firebase": "^5.6.0",
    "firebase-admin": "^6.3.0",
    "http-errors": "~1.7.1",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.3.0",
    "koogn": "^0.1.0",
    "logdna-bunyan": "^1.0.1",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.41.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.7",
    "firebase-functions-test": "^0.1.5",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^7.1",
    "gulp-typescript": "^3.1.6",
    "source-map-support": "^0.4.15",
    "tslint": "^4.5.1",
    "tslint-eslint-rules": "^3.5.1",
    "typescript": "^2.2.1",
    "mocha": "^5.2.0",
    "chai": "^4.2.0",
    "sinon": "^7.1.1",
    "nyc": "^13.1.0",
    "nodemon": "^1.18.4"
  },
  "author": "<gvko@pm.me>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/galioy/forro-cph/issues"
  },
  "homepage": "https://github.com/galioy/forro-cph#readme"
}
